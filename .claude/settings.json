{
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(npm list:*)",
      "Bash(npm run test:*)",
      "Bash(npx playwright test:*)",
      "Bash(npx vitest run:*)",
      "Bash(npx vitest:*)",
      "WebSearch",
      "WebFetch(domain:nuxt.com)",
      "WebFetch(domain:ui4.nuxt.com)",
      "WebFetch(domain:ui.nuxt.com)",
      "Bash(npx wrangler d1 execute=:* --remote=:* --command :*SELECT:*)"
    ],
    "deny": [
      "Read(./.cookies.txt)",
      "Read(./.env)",
      "Read(./.env.dev)",
      "Read(./.env.prod)",
      "Bash(npx wrangler :* --remote :*DELETE:*)",
      "Bash(npm run dev:*)",
      "Bash(git stash:*)"
    ],
    "ask": [
      "Bash(npx wrangler :* --remote :*)"
    ],
    "includeCoAuthoredBy": false
  }
}
